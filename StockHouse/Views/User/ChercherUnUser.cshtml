@model StockHouse.Models.User

@{
    ViewBag.Title = "ChercherUnUser";
}

<h2>ChercherUnUser</h2>

@using (Html.BeginForm("ModifierUnUser", "User", FormMethod.Post))
{
    <div class="form-group">
        <label>Id:</label>
        <br/>
        @Html.TextBoxFor(p => p.Id)
        <br/>
        @Html.ValidationMessageFor(p => p.Id)
    </div>
    <button type="submit" class="btn btn-primary">Chercher</button>
}
